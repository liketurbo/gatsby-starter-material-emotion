{
  "name": "gatsby-starter-material-ui",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and Material UI",
  "version": "0.1.0",
  "author": "Dominic Abela <djmabela@gmail.com>",
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.0.1",
    "gatsby": "^2.7.5",
    "gatsby-plugin-material-ui": "^2.0.1",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-typescript": "^2.1.26",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-get-off-my-lawn": "^5.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -p 3000",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dominicabela/gatsby-starter-material-ui"
  },
  "bugs": {
    "url": "https://github.com/dominicabela/gatsby-starter-material-ui/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
