{
  "name": "gatsby-starter-material-emotion",
  "private": true,
  "description": "A simple Gatsby starter to get up and developing quickly with Material UI and Emotion",
  "version": "1.0.0",
  "author": "Ramzan Chasygov <theliketurbo@gmail.com>",
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@material-ui/core": "4.9.10",
    "@material-ui/icons": "4.9.1",
    "@material-ui/styles": "4.9.10",
    "emotion-theming": "10.0.27",
    "gatsby": "2.20.24",
    "gatsby-image": "2.3.4",
    "gatsby-plugin-emotion": "4.2.2",
    "gatsby-plugin-manifest": "2.3.5",
    "gatsby-plugin-material-ui": "2.1.6",
    "gatsby-plugin-react-helmet": "3.2.4",
    "gatsby-plugin-sharp": "2.5.6",
    "gatsby-plugin-typescript": "2.3.3",
    "gatsby-source-filesystem": "2.2.4",
    "gatsby-transformer-sharp": "2.4.6",
    "keyword-extractor": "0.0.18",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@types/react-helmet": "5.0.15",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-get-off-my-lawn": "5.0.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-simple-import-sort": "5.0.2",
    "husky": "4.2.5",
    "lint-staged": "10.1.4",
    "prettier": "2.0.4",
    "typescript": "3.9.6"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "emotion",
    "material-ui",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop -p 3000",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liketurbo/gatsby-starter-material-emotion"
  },
  "bugs": {
    "url": "https://github.com/liketurbo/gatsby-starter-material-emotion/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
